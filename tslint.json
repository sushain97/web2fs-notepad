{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "@blueprintjs/tslint-config/blueprint-rules",
    "tslint-config-prettier",
    "tslint-sonarts"
  ],
  "rules": {
    "member-ordering": {
      "options": {
        "alphabetize": true,
        "order": "instance-sandwich"
      }
    },
    "no-nested-template-literals": {
      "severity": "off"
    }
  }
}
