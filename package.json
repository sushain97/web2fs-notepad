    "tsc": "tsc --noEmit",
    "lint": "yarn run eslint && yarn run prettier --list-different && yarn run stylelint && yarn run tsc",
    "fix": "yarn run eslint --fix && yarn run prettier --write && yarn run stylelint --fix"
